<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">
    <font
        android:font="@font/comfortaa_bold"
        android:fontStyle="normal"
        android:fontWeight="800"
        app:font="@font/comfortaa_bold"
        app:fontStyle="normal"
        app:fontWeight="800" />
    <font
        android:font="@font/comfortaa_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/comfortaa_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />
    <font
        android:font="@font/comfortaa_light"
        android:fontStyle="normal"
        android:fontWeight="100"
        app:font="@font/comfortaa_light"
        app:fontStyle="normal"
        app:fontWeight="100" />
</font-family>